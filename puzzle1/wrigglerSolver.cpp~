#include "wrigglerSolver.h"

void WrigglerSolver::solveBFTS(Puzzle p)
{
  m_frontier.push(p);
  
  while(!m_frontier.front().isSolved())
  {
    //test each direction
    for(int i = 0; i < 4; i++)
    {
      m_frontier.push(p.moveHead(
    }
  }
}

